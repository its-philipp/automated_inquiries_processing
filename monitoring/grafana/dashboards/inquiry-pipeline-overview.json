{
  "title": "Inquiry Processing Pipeline Overview",
  "panels": [
    {
      "title": "Inquiry Submissions",
      "type": "stat",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "targets": [
        {
          "expr": "http_request_duration_seconds_count{endpoint=\"/api/v1/inquiries/submit\"}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Inquiries Processed",
      "type": "stat", 
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
      "targets": [
        {
          "expr": "inquiry_processing_duration_seconds_count",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Routing Decisions by Department",
      "type": "piechart",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
      "targets": [
        {
          "expr": "routing_decisions_total",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Model Inferences",
      "type": "table",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
      "targets": [
        {
          "expr": "model_inferences_total",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Processing Duration Over Time",
      "type": "graph",
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 16},
      "targets": [
        {
          "expr": "rate(inquiry_processing_duration_seconds_count[5m])",
          "refId": "A"
        }
      ]
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "refresh": "5s"
}